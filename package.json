{
  "name": "vite-npm-package",
  "version": "1.0.0",
  "description": "Vite JavaScript library npm package template",
  "author": "Jason Sturges <jason@jsonsturges.com> (https://jasonsturges.com)",
  "homepage": "https://github.com/jasonsturges/vite-npm-package",
  "repository": "github:jasonsturges/vite-npm-package",
  "license": "ISC",
  "keywords": [
    "vite",
    "javascript",
    "npm",
    "package"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "start": "vite --host --open",
    "build": "vite build",
    "format": "prettier --write \"src/**/*.{js}\""
  },
  "dependencies": {
    "@turf/turf": "6.5.0",
    "@types/numeral": "^2.0.5",
    "numeral": "^2.0.6"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "vite": "^5.1.4"
  },
  "peerDependencies": {
    "@mapbox/mapbox-gl-draw": "^1.4.3"
  }
}
